{
  "name": "talos-backend-node",
  "version": "1.0.0",
  "description": "Talos Backend API Server (Node.js version)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "lint": "eslint . --ignore-pattern node_modules",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "services/**/*.js",
      "routes/**/*.js"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^169.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pdf-parse": "^2.1.7",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "better-sqlite3": "^12.4.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "keywords": [
    "talos",
    "hvac",
    "hiring",
    "api"
  ],
  "author": "Talos",
  "license": "MIT"
}
